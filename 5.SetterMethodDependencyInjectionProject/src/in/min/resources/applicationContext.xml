<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    
    <bean class="in.min.beans.Address" id="addrId">
    <property name="houseNo" value="95"/>
    <property name="city" value="Delhi"/>
    <property name="pinCode" value="110025"/>
    
    </bean>
    
    <bean class="in.min.beans.Student" id="stdId">
    <property name="name" value="Fattu"/>
    <property name="rollNo" value="101"/>
    <property name="address" ref="addrId"/>
    
    </bean>

</beans>