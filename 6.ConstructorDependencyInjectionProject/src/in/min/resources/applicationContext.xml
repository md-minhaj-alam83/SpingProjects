<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    
    <bean class="in.min.beans.Address" id="addrId">
    <constructor-arg value="95"/>
    <constructor-arg value="Delhi"/>
    <constructor-arg value="110025"/>
    
    </bean>
    
    <bean class="in.min.beans.Student" id="stdId">
    <constructor-arg value="Fattu"/>
    <constructor-arg value="101"/>
    <constructor-arg ref="addrId"/>
    
    
    </bean>

</beans>